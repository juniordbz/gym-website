{
	"printWidth": 80,
	"tabWidth": 2,
	"singleQuote": true,
	"trailingComma": false,
	"arrowParens": false,
	"semi": false,
	"endOfLine": false,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
